<!DOCTYPE html>
<html>
@Html.Partial("Partials/_HeaderPartial")
<body style="margin-top:0;">
    @Html.Partial("Partials/_NavBarPartial")

    <div class="container">
        <div class="row" style="margin-top:120px;">
            <div class="col-sm-3">
                <ul class="nav nav-sidebar">
                    <li>@Html.ActionLink("Manage Accounts", "ManageAccounts", "Director", routeValues: null, htmlAttributes: new { id = "ManageAccountsLink" })</li>
                    <li>@Html.ActionLink("Manage Site", "ManageSite", "Director", routeValues: null, htmlAttributes: new { id = "ManageSiteLink" })</li>
                    <li>@Html.ActionLink("Manage Personal Account", "ManagePersonalAccount", "Director", routeValues: null, htmlAttributes: new { id = "ManagePersonalAccountLink" })</li>
                </ul>
            </div>
            <div class="col-sm-9">
                @RenderBody()
            </div>
        </div>
    </div>
    <div>
        <div class="divide20"></div>
    </div>

    @Html.Partial("Partials/_FooterPartial")
    @Html.Partial("Partials/_ValidationScriptsPartial")

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")

    @RenderSection("scripts", required: false)
</body>
</html>
